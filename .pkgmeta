package-as: AzeriteUI
enable-nolib-creation: no

optional-dependencies:
    - clique

move-folders:
    AzeriteUI/AzeriteUI: AzeriteUI

manual-changelog:
    filename: CHANGELOG.md
    markup-type: markdown

license-output: LICENSE.txt

ignore:
    - AzeriteUI/back-end/external
    - AzeriteUI/back-end/external.xml
    - .pkgmeta-classic
    - CHANGELOG.md
    - CODE_OF_CONDUCT.md
    - LICENSE.md
    - README.md
    - .github
    